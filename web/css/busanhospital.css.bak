body {
  font-family: 'Noto Sans KR', sans-serif;
	font-family: 'NanumGothic', sans-serif;
  font-weight: 400;
}

.blueline_pop {background-color: #0284cf;}
#main-page-1 {
  background-image: url('../img/main_02.jpg');
  padding: 6% 0 0 0;
  height: 750px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}



#navbar a {color:#333333; font-size:17px; font-weight: 500;}

#merit img {border:1px solid #c4c4c4;}
#merit .con { color:#333333; font-size:16px;}
#merit h5 {font-family:'Noto Sans KR',sans-serif; font-weight: 500;}

#kidney {
  background-image: url('../img/bg_kidney.jpg');
	 padding: 6% 0 0 0;
  height:327px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

#kidney hr {border-top: 1px solid #fff; border-bottom:none;}
#kidney p {}

#callnum {background-color: #2f4459;}
.call {font-size:25px;}
.call_num {font-family: 'Noto Sans KR', sans-serif;  font-weight: 700; font-size:35px; color: #ff9d32; }

#notice h3 {font-family:'Noto Sans KR',sans-serif; font-weight: 500;}
#notice h3 {color:#333333;}
#notice hr {margin:5px 0;}
#notice img {margin-top:-5px;}

#info {font-family:'Noto Sans KR',sans-serif; }
.bg_mint {background-color: #60beb9;}
#info hr {border-top: 1px solid #fff; border-bottom:none; position: relative; top:-15px;}
.bg_lightblue {background-color:#3378c1;}
.bg_sky {background-color: #00aeef;}
.bg_navy {background-color: #004a80;}
.bg_navy h1 {font-weight:700; font-size:50px;}
.bg_deepgray {background-color: #707070;}
#info p, span {font-size:40px;}

#gallery img {margin-top:-4px;}
.img-fluid img{
  max-width: 100%;
  height: auto;
}

#gallery span {position:relative; top:100px; font-size:18px; font-weight:700; color:#787878;}

.text-333 {color:#333333;}
.text-00adff {color:#00adff;}

#with span {font-family: 'Noto Sans KR', sans-serif; font-size:25px; font-weight:500;}